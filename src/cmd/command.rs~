use crate::Buffer;

// A command abstraction
pub enum Cmd<'a> {
    A(Append<'a>),
}
struct Append<'a> {
    buffer: &'a dyn Buffer,
    selection: (usize, usize),
    input: Vec<String>,
    // Flags
    p: bool,
}
